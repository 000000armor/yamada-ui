{
  "name": "@yamada-ui/react",
  "version": "0.1.0",
  "description": "React UI components of the Yamada, by the Yamada, for the Yamada built with React and Emotion",
  "keywords": [],
  "author": "hirotomoyamada <hirotomo.yamada@avap.co.jp>",
  "license": "MIT",
  "main": "src/index.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "pnpm build:fast -- --watch",
    "build": "JSX=1 tsup src/index.ts --dts",
    "build:fast": "JSX=1 tsup src/index.ts",
    "clean": "rimraf dist .turbo",
    "typecheck": "tsc --noEmit",
    "prepack": "clean-package",
    "postpack": "clean-package restore"
  },
  "dependencies": {
    "@yamada-ui/components": "workspace:*",
    "@yamada-ui/core": "workspace:*",
    "@yamada-ui/data-display": "workspace:*",
    "@yamada-ui/feedback": "workspace:*",
    "@yamada-ui/forms": "workspace:*",
    "@yamada-ui/hooks": "workspace:*",
    "@yamada-ui/layouts": "workspace:*",
    "@yamada-ui/media-and-icons": "workspace:*",
    "@yamada-ui/motion": "workspace:*",
    "@yamada-ui/navigation": "workspace:*",
    "@yamada-ui/overlay": "workspace:*",
    "@yamada-ui/providers": "workspace:*",
    "@yamada-ui/theme": "workspace:*",
    "@yamada-ui/transitions": "workspace:*",
    "@yamada-ui/typography": "workspace:*",
    "@yamada-ui/utils": "workspace:*"
  },
  "devDependencies": {
    "clean-package": "2.1.1",
    "react": "^18.0.0"
  },
  "peerDependencies": {
    "react": ">=18"
  }
}
