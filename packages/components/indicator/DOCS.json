{
  "Indicator": {
    "colorScheme": {
      "defaultValue": "primary",
      "type": "\"whiteAlpha\" | \"blackAlpha\" | \"gray\" | \"neutral\" | \"red\" | \"rose\" | \"pink\" | \"flashy\" | \"orange\" | \"amber\" | \"yellow\" | \"lime\" | \"green\" | \"emerald\" | \"teal\" | \"cyan\" | \"sky\" | \"blue\" | \"indigo\" | \"violet\" | \"purple\" | \"fuchsia\"",
      "required": false,
      "description": "The visual color appearance of the component."
    },
    "inline": {
      "type": "UIValue<boolean>",
      "defaultValue": false,
      "required": false,
      "description": "If `true`, set the indicator as an inline element."
    },
    "isDisabled": {
      "type": "boolean",
      "defaultValue": false,
      "required": false,
      "description": "If `true`, the indicator will be disabled."
    },
    "label": {
      "type": "type ONLY_FOR_FORMAT =\n  | string\n  | number\n  | boolean\n  | ReactElement<any, string | JSXElementConstructor<any>>\n  | Iterable<ReactNode>\n  | ReactPortal",
      "required": false,
      "description": "The indicator label to use."
    },
    "offset": {
      "type": "UIValue<number>",
      "defaultValue": "0",
      "required": false,
      "description": "Changes position offset, usually used when element has border-radius."
    },
    "overflowCount": {
      "type": "number",
      "defaultValue": "99",
      "required": false,
      "description": "If `label` is of type number, the maximum number displayed."
    },
    "placement": {
      "type": "UIValue<\n  | \"top\"\n  | \"right\"\n  | \"bottom\"\n  | \"left\"\n  | \"top-left\"\n  | \"top-right\"\n  | \"bottom-left\"\n  | \"bottom-right\"\n>",
      "defaultValue": "'top-right'",
      "required": false,
      "description": "The placement of the indicator."
    },
    "showZero": {
      "type": "boolean",
      "defaultValue": true,
      "required": false,
      "description": "If `true`, display 0."
    },
    "size": {
      "defaultValue": "md",
      "type": "\"sm\" | \"md\" | \"lg\"",
      "required": false,
      "description": "The size of the Indicator."
    },
    "variant": {
      "defaultValue": "solid",
      "type": "\"solid\" | \"subtle\"",
      "required": false,
      "description": "The variant of the Indicator."
    },
    "withBorder": {
      "type": "boolean",
      "defaultValue": false,
      "required": false,
      "description": "If `true`, display the border of the indicator."
    }
  }
}
