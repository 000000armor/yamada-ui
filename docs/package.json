{
  "name": "docs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "start": "next start",
    "build": "next build",
    "build:profile": "next build --profile",
    "gen:changelog": "tsx scripts/changelog",
    "gen:theme": "tsx scripts/theme-component",
