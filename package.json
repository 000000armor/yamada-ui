{
  "name": "yamada-ui",
  "version": "0.1.0",
  "engines": {
    "node": "18"
  },
  "license": "MIT",
  "author": "hirotomoyamada <hirotomo.yamada@avap.co.jp>",
  "scripts": {
    "prepare": "husky install",
    "postinstall": "cd packages/cli && pnpm build",
    "clean": "pnpm -r --parallel exec rimraf dist .turbo *.log",
    "lint": "eslint --max-warnings 0 .",
    "format": "prettier --write .",
    "dev": "turbo run dev --filter=!./examples/*",
    "sb": "start-storybook -p 6006",
    "sb:serve": "pnpx serve storybook-static",
    "prebuild": "pnpm gen:tokens",
    "build": "turbo run build --filter=!./examples/*",
    "build:fast": "turbo run build:fast --filter=!./examples/*",
    "build:sb": "pnpm build && build-storybook",
    "gen:tokens": "yamada-cli tokens packages/theme/src --out packages/core/src/generated-theme.types.ts",
    "gen:component": "plop component && pnpm install",
    "gen:hook": "plop hook && pnpm install",
    "next:dev": "pnpm --prefix examples/next-js dev",
    "next:build": "pnpm --prefix examples/next-js build",
    "next:start": "pnpm --prefix examples/next-js start",
    "react:start": "pnpm --prefix examples/create-react-app start",
    "react:build": "pnpm --prefix examples/create-react-app build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-storysource": "^6.5.16",
    "@storybook/addons": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/cli": "^6.5.16",
    "@storybook/core-common": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/theming": "^6.5.16",
    "@types/node": "20.1.1",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "@yamada-ui/cli": "workspace:*",
    "@yamada-ui/react": "workspace:*",
    "eslint": "8.40.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "5.10.3",
    "find-packages": "10.0.4",
    "husky": "^8.0.3",
    "lint-staged": "13.2.2",
    "ora": "^5.4.1",
    "plop": "3.1.2",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-loader-spinner": "^5.3.4",
    "rimraf": "5.0.0",
    "storybook-dark-mode": "^3.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "tsup": "6.7.0",
    "tsx": "^3.12.7",
    "turbo": "1.9.3",
    "typescript": "4.9.4"
  },
  "pnpm": {
    "overrides": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  }
}
