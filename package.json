{
  "name": "yamada-ui",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": "18"
  },
  "license": "MIT",
  "author": "hirotomoyamada <hirotomo.yamada@avap.co.jp>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hirotomoyamada/yamada-ui"
  },
  "scripts": {
    "prepare": "husky install",
    "postinstall": "cd packages/cli && pnpm build",
    "clean": "pnpm -r --parallel exec rimraf dist .turbo *.log",
    "lint": "eslint --max-warnings 0 .",
    "format": "prettier --write .",
    "dev": "turbo run dev --filter=!./examples/*",
    "test": "jest",
    "sb": "start-storybook -p 6006",
    "sb:serve": "pnpx serve storybook-static",
    "prebuild": "pnpm gen:tokens",
    "build": "turbo run build --filter=!./examples/*",
    "build:fast": "turbo run build:fast --filter=!./examples/*",
    "build:sb": "pnpm build && build-storybook",
    "gen:tokens": "yamada-cli tokens packages/theme/src/index.ts --out packages/core/src/generated-theme.types.ts",
    "gen:component": "plop component && pnpm install",
    "gen:hook": "plop hook && pnpm install",
    "changelog": "tsx scripts/changelog.ts",
    "changelog:latest": "tsx scripts/changelog.ts --latest",
    "changelog:commit": "git add . && git commit -am 'docs: add changelog' && git push",
    "changelog:format": "prettier --write .changelog",
    "react:start": "pnpm --prefix examples/create-react-app start",
    "react:build": "pnpm --prefix examples/create-react-app build",
    "next-pages:dev": "pnpm --prefix examples/next-js-pages dev",
    "next-pages:build": "pnpm --prefix examples/next-js-pages build",
    "next-pages:start": "pnpm --prefix examples/next-js-pages start",
    "next-app:dev": "pnpm --prefix examples/next-js-app dev",
    "next-app:build": "pnpm --prefix examples/next-js-app build",
    "next-app:start": "pnpm --prefix examples/next-js-app start"
  },
  "dependencies": {
    "@babel/core": "^7.22.1",
    "@babel/eslint-parser": "^7.21.8",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.22.2",
    "@babel/preset-react": "^7.22.3",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/runtime": "^7.22.3",
    "@changesets/changelog-github": "0.4.8",
    "@changesets/cli": "2.26.1",
    "@changesets/get-release-plan": "3.0.16",
    "@changesets/types": "5.2.1",
    "@commitlint/cli": "17.6.3",
    "@commitlint/config-conventional": "17.6.3",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@faker-js/faker": "^8.0.1",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-regular-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@octokit/rest": "^19.0.11",
    "@storybook/addon-docs": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-storysource": "^6.5.16",
    "@storybook/addons": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/cli": "^6.5.16",
    "@storybook/core-common": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.1.0",
    "@storybook/theming": "^6.5.16",
    "@swc-node/jest": "^1.6.5",
    "@swc/core": "^1.3.60",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/jest": "^29.5.1",
    "@types/node": "20.2.5",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@typescript-eslint/eslint-plugin": "5.59.7",
    "@typescript-eslint/parser": "5.59.7",
    "@yamada-ui/cli": "workspace:*",
    "@yamada-ui/react": "workspace:*",
    "@yamada-ui/test": "workspace:*",
    "eslint": "8.41.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "5.11.0",
    "find-packages": "^10.0.4",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-watch-typeahead": "2.2.2",
    "lint-staged": "13.2.2",
    "ora": "^6.3.1",
    "plop": "3.1.2",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "react-loader-spinner": "^5.3.4",
    "rimraf": "5.0.1",
    "storybook-dark-mode": "^3.0.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.2",
    "tsup": "6.7.0",
    "tsx": "^3.12.7",
    "turbo": "1.9.9",
    "typescript": "4.9.4"
  },
  "pnpm": {
    "overrides": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  }
}
